# Copy this file to `.env` in the repo root before running docker compose.
#
# Ports on your host machine
FRONTEND_PORT=3000
BACKEND_PORT=3001

# Frontend reads this (client + server)
NEXT_PUBLIC_BACKEND_API_URL=http://localhost:3001

# Backend CORS: comma-separated list of allowed origins
ALLOWED_URLS=http://localhost:3000

# Notion credentials (required for backend)
NOTION_TOKEN=your_notion_token_here
NOTION_DATA_SOURCE_ID=your_notion_data_source_id_here

# Optional
MAX_FILTER_DEPTH=2
